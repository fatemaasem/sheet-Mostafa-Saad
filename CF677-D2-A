#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
#include <cstring>
/* run this program using the console pauser or add your own getch, system("pause") or input loop */
 
int main(int argc, char** argv) {
	int x;
	cin>>x;
	int h;
	cin>>h;
	int sum=0;
	for(int i=0;i<x;i++){
		int v;
		cin>>v;
		if(v>h)
		sum+=2;
		else
		sum++;
	}
	cout<<sum<<"\n";
	return 0;
}
